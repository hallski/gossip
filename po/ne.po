msgid "new account"
msgstr "नयाँ खाता"

#: ../libgossip/gossip-jabber.c:536
msgid "Home"
msgstr "गृह"

#: ../libgossip/gossip-jabber.c:1384 ../libgossip/gossip-jabber.c:1523
msgid "Couldn't send message!"
msgstr "सन्देश पठाउन सकिएन !"

#: ../libgossip/gossip-jabber.c:1470
msgid "Connection could not be opened"
msgstr "जडान खोल्न सकिँदैन"

#: ../libgossip/gossip-jabber.c:3885
msgid "An unknown error occurred."
msgstr "एउटा अज्ञात त्रुटि देखापर्यो ।"

#: ../libgossip/gossip-jabber.c:3889
msgid "Connection refused."
msgstr "जडान अस्वीकृत भयो ।"

#: ../libgossip/gossip-jabber.c:3892
msgid "Server address could not be resolved."
msgstr "सर्भर ठेगाना पुन: समाधान हुन सक्दैन।"

#: ../libgossip/gossip-jabber.c:3895
msgid "Connection timed out."
msgstr "जडान समय समाप्त भयो ।"

#: ../libgossip/gossip-jabber.c:3898
msgid "Authentication failed."
msgstr "प्रमाणीकरण असफल भयो ।"

#: ../libgossip/gossip-jabber.c:3901
msgid "The username you are trying already exists."
msgstr "तपाईँले प्रयास गर्नुभएको प्रयोगकर्ता नाम पहिल्यै अवस्थित छ।"

#: ../libgossip/gossip-jabber.c:3904
msgid "The username you are trying is not valid."
msgstr "तपाईँले प्रयास गर्नुभएको प्रयोगकर्ता नाम वैध छैन ।"

#: ../libgossip/gossip-jabber.c:3907
msgid "This feature is unavailable."
msgstr "यो विशेषता उपलब्ध छैन ।"

#: ../libgossip/gossip-jabber.c:3910
msgid "This feature is unauthorized."
msgstr "यो विशेषता प्रमाणीकरण गरिएको छैन ।"

#: ../libgossip/gossip-jabber.c:3913
msgid "A specific protocol error occurred that was unexpected."
msgstr "एउटा अनपेक्षित प्रोटोकल त्रुटि देखा पर्यो ।"

#: ../libloudermouth/lm-bs-receiver.c:90 ../libloudermouth/lm-bs-sender.c:251
msgid "The other party disconnected"
msgstr "अन्य पक्ष विच्छेदन भयो"

#: ../libloudermouth/lm-bs-receiver.c:116
msgid "A protocol error occurred during the transfer"
